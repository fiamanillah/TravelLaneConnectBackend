#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/esbuild@0.14.47/node_modules/esbuild/bin/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/esbuild@0.14.47/node_modules/esbuild/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/esbuild@0.14.47/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/esbuild@0.14.47/node_modules/esbuild/bin/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/esbuild@0.14.47/node_modules/esbuild/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/esbuild@0.14.47/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
