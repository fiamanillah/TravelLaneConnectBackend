#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/vercel@39.4.2/node_modules/vercel/dist/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/vercel@39.4.2/node_modules/vercel/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/vercel@39.4.2/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/vercel@39.4.2/node_modules/vercel/dist/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/vercel@39.4.2/node_modules/vercel/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/vercel@39.4.2/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/vc.js" "$@"
else
  exec node  "$basedir/../../dist/vc.js" "$@"
fi
