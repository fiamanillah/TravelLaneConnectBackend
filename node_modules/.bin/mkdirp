#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/mkdirp/bin/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/mkdirp/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/node_modules:/mnt/nvme1n1p3/Web Dev/node_modules:/mnt/nvme1n1p3/node_modules:/mnt/node_modules:/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/mkdirp/bin/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/mkdirp/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/node_modules:/mnt/nvme1n1p3/Web Dev/node_modules:/mnt/nvme1n1p3/node_modules:/mnt/node_modules:/node_modules:/mnt/nvme1n1p3/Web Dev/TravelLaneConnect/TravelLaneConnectBackend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../mkdirp/bin/cmd.js" "$@"
fi
